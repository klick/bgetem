{
  "require": {
    "besteadfast/craft-preparse-field": "^2.0",
    "bitmatrix/craft-omnisearch": "^2.0",
    "born05/craft-assetusage": "^3.1",
    "by/first-image": "^2.0",
    "chasegiunta/scss": "^2.0",
    "clubstudioltd/craft-asset-rev": "^7.0",
    "craftcms/cms": "^4.4.0",
    "craftcms/commerce": "^4.2",
    "craftcms/element-api": "3.0.1.1",
    "craftcms/feed-me": "5.1.1.1",
    "craftcms/redactor": "^3.0",
    "craftsnippets/craft-image-toolbox": "^2.0",
    "ether/tags": "^2.0",
    "mdxdave/time-ago": "^1.1",
    "mmikkel/retcon": "^2.6",
    "nystudio107/craft-emptycoalesce": "^4.0",
    "nystudio107/craft-seomatic": "^4.0",
    "nystudio107/craft-similar": "*",
    "pdaleramirez/super-filter": "^1.7",
    "percipioglobal/craft-colour-swatches": "^4.3",
    "putyourlightson/craft-blitz": "^4.4",
    "sebastianlenz/linkfield": "^2.1",
    "spacecatninja/imager-x": "^4.1",
    "spicyweb/craft-neo": "^3.7",
    "topshelfcraft/supersort": "^4.0",
    "topshelfcraft/wordsmith": "^4.2",
    "verbb/comments": "^2.0",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
