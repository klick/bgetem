{% set jsonLdContainer = seomatic.jsonLd.container() %}
{% do jsonLdContainer.include(false) %}
{% set scriptContainer = seomatic.script.container() %}
{% do scriptContainer.include(false) %}

<!DOCTYPE html>
<html lang="de">
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta charset="UTF-8">
		<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
		<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
		<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
		<link rel="manifest" href="/site.webmanifest">
		<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#004993">
		<link rel="shortcut icon" href="/favicon.ico">
		<meta name="msapplication-TileColor" content="#ffffff">
		<meta name="msapplication-config" content="/browserconfig.xml">
		<meta name="theme-color" content="#ffffff">
		<meta name="robots" content="noindex,nofollow">	
		{% hook 'seomaticRender' %}
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

		<!-- JavaScript Bundle with Popper -->
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
		<link rel="stylesheet" href="{{ rev('css/styles.css') }}">
<link rel="stylesheet" href="/css/prism.css">
<link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>

		

		{# <link href="http://mmenu.localhost/dist/mmenu.css" rel="stylesheet" /> #}
	</head>
	<body class="" data-content-type="">



		<div id="my-page">
			{% block content %}
			<p>If you see me, you haven’t set your <code>{% verbatim %}{% block content %}…{% endblock %}{% endverbatim %}</code> yet.</p>
			<p>See Twig’s <a href="http://twig.sensiolabs.org/doc/templates.html#template-inheritance">Template Inheritance</a> documentation for an explanation.</p>
			{% endblock %}
		</div>





		<script src="https://code.jquery.com/jquery-2.2.4.min.js" type="text/javascript"></script>
		<script src="js/popper.min.js"></script>
		<script src="js/bootstrap.min.js"></script>
		

		<script>
		$(document).ready(function() {
			//alert('shown'); 
			$( ".navbar-toggler-word" ).on( "click", function() {
    			console.log( "hamburger show" );
    			$('.navbar-collapse').toggleClass('show');
    			$('.nav-wrapper').toggleClass('open');
			} )
			.on( "hide.bs.collapse", function() {
    			//alert( "hamburger hide" );
			} );
			$( "small.toggle-code" ).on( "click", function() {
				//alert( "hamburger hide" );
				$(this).toggleClass("open");
				$(this).parents('div.container').find('pre').toggleClass("visible");
			})
		});

		</script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.20.0/prism.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.20.0/plugins/autoloader/prism-autoloader.min.js"></script>
<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
	<script src="js/bundle.js"></script>

		<script>
			
		  $(document).ready(function(){
		  	$('.slick-slider-module-gallery').slick({
		    	fade: false,
				mobileFirst: true,
				variableWidth: false,
				arrows: false,
				autoplay: true,
				autoplaySpeed: 5000,
				prevArrow:$('.previ_slider'),
		  		nextArrow:$('.nexti_slider'),
				infinite: true,
				  	responsive: [
		  		{
			      breakpoint: 1,
			      settings: {
			        arrows: false,
			        //centerMode: true,
			        //centerPadding: '40px',
			        slidesToShow: 1,
			        slidesToScroll: 1
			      }
			    },
			    {
			      breakpoint: 600,
			      settings: {
			        arrows: false,
			        //centerMode: true,
			        //centerPadding: '40px',
			        slidesToShow: 1,
			        slidesToScroll: 1
			      }
			    },
			    {
			      breakpoint: 992,
			      settings: {
			        arrows: true,
			        //centerMode: true,
			        //centerPadding: '0px',
			        slidesToShow: 1,
			        slidesToScroll: 1
			      }
			    },
			    {
			      breakpoint: 1200,
			      settings: {
			        arrows: true,
			        //centerMode: true,
			        //centerPadding: '80px',
			        slidesToShow: 1,
			        slidesToScroll: 1
			      }
			    }
			  ]
		  });
		});

		</script>


	</body>
</html>